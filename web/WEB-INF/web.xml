<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ActiviteServelet</servlet-name>
        <servlet-class>servelet.ActiviteServelet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CategorieServelet</servlet-name>
        <servlet-class>servelet.CategorieServelet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BouquetServelet</servlet-name>
        <servlet-class>servelet.BouquetServelet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BouquetActivite</servlet-name>
        <servlet-class>servelet.BouquetActivite</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BouquetActiviteServelet</servlet-name>
        <servlet-class>servelet.BouquetActiviteServelet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ActiviteServelet</servlet-name>
        <url-pattern>/ActiviteServelet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CategorieServelet</servlet-name>
        <url-pattern>/CategorieServelet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BouquetServelet</servlet-name>
        <url-pattern>/BouquetServelet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BouquetActivite</servlet-name>
        <url-pattern>/BouquetActivite</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BouquetActiviteServelet</servlet-name>
        <url-pattern>/BouquetActiviteServelet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
